<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.dfs-golf.optimizer</string>

    <key>ProgramArguments</key>
    <array>
        <string>/bin/bash</string>
        <string>/Users/rhbot/Desktop/dfs-golf/run_optimizer.sh</string>
    </array>

    <!-- Run every Thursday at 3:00 AM CST -->
    <key>StartCalendarInterval</key>
    <dict>
        <key>Weekday</key>
        <integer>4</integer>
        <key>Hour</key>
        <integer>3</integer>
        <key>Minute</key>
        <integer>0</integer>
    </dict>

    <key>StandardOutPath</key>
    <string>/Users/rhbot/Desktop/dfs-golf/history/launchd_stdout.log</string>

    <key>StandardErrorPath</key>
    <string>/Users/rhbot/Desktop/dfs-golf/history/launchd_stderr.log</string>

    <key>WorkingDirectory</key>
    <string>/Users/rhbot/Desktop/dfs-golf</string>
</dict>
</plist>
